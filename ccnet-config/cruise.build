<project name="ccnetlaunch" default="run">
  <target name="run" depends="update,build"/>

  <target name="update">
    <exec basedir=".." program="tools/cvs.exe" commandline="-q update -P -d" />
  </target>

  <target name="build">
    <nant buildfile="..\ccnet.build" target="all.ccnet" />
  </target>

</project>