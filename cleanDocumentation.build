<?xml version="1.0" encoding="utf-8"?>

<project name="CruiseControl.NET" default="all" xmlns="http://nant.sf.net/release/0.86-beta1/nant.xsd">
  <target name="all">
    <delete>
      <fileset basedir="." failonempty="false">
        <include name="doc-*.log"/>
      </fileset>
    </delete>
    <copy file="${path::get-full-path('pages.xml')}" tofile="${path::combine( path::get-full-path('wiki'), 'pages.xml')}"/>
  </target>
</project>
