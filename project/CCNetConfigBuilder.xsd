<?xml version="1.0" encoding="utf-8"?>
<xs:schema targetNamespace="urn:ccnet.config.builder"
           elementFormDefault="qualified"
           xmlns="urn:ccnet.config.builder"
           xmlns:xs="http://www.w3.org/2001/XMLSchema">
